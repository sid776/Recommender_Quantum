header: Rename columns in riskfactor table
query: 
    alter table {catalog}.ussparc_silver.riskfactor rename column ALT_NAME to ALT_RF_NM;
    alter table {catalog}.ussparc_silver.riskfactor rename column RF_CLASS to RF_CLASS_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column CURRENCY to CURRENCY_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column CURVE_TYPE to CURVE_INSTRUMENT_TYPE_NM;
    alter table {catalog}.ussparc_silver.riskfactor rename column EFFECTIVE_CURRENCY to EFFECTIVE_CURRENCY_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column FUTURE_TENOR to FUTURE_TENOR_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column INDEX_TENOR to INDEX_TENOR_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column INST_TYPE to INST_TYPE_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column CLEARING_HOUSE to CLEARING_HOUSE_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column MARGIN_TYPE to MARGIN_TYPE_NM;
    alter table {catalog}.ussparc_silver.riskfactor rename column SHOCK_TYPE to SHOCK_TYPE_CD;
    alter table {catalog}.ussparc_silver.riskfactor rename column VALID_FROM to VALID_FROM_TS;
    alter table {catalog}.ussparc_silver.riskfactor rename column VALID_TO to VALID_TO_TS;

    
